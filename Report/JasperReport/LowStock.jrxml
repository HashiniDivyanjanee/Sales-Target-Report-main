	<parameter name="ParameterSupplier" class="java.lang.String"/>
	<parameter name="ParameterCategory" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT 
    i.Item_ID,
    i.Item_Name,
    i.Cat_Name,
    i.ItemQty,
    i.Reorder_Level,
    i.Reorder_Qty,
    isl.Supp_Name AS Item_Supp_Name,
    s.Supp_Name,
    (i.Reorder_Level - i.ItemQty) + i.Reorder_Qty AS Reorder_Amount
FROM 
    items i
JOIN 
    items_supp_list isl
ON 
    i.Item_ID = isl.Item_ID
JOIN 
    supplier s
ON 
    isl.Supp_Name = s.Supp_CustomID
WHERE 
    i.Reorder_Qty > i.ItemQty
    AND (s.Supp_Name = $P{ParameterSupplier} and i.Cat_Name = $P{ParameterCategory});]]>
	</queryString>
